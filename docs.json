{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "mint",
    "name": "Gang",
    "description": "The fastest way to trade memecoins with your gang",
    "colors": {
         "primary": "#000000",
         "light": "#ffffff",
         "dark": "#ffffff"
    },
    "appearance": {
        "default": "dark",
        "strict": true
    },
    "background": {
       "decoration": "grid",
       "color": {"dark": "#000000", "light": "#000000"}
    },
    "favicon": "./favicon.png",
    "navigation": {
        "pages": [
            "index"
        ],
        "global": {
            "anchors": [
                {
                    "anchor": "Documentation",
                    "href": "https://docs.gang.trade",
                    "icon": "book-open-cover"
              
                },
                {
                    "anchor": "Community",
                    "href": "https://discord.gg/tradewithgang",
                    "icon": "discord"
                },
                {
                    "anchor": "Trade",
                    "href": "https://t.me/tradewithgang_bot",
                    "icon": "telegram"
                }
            ]
        }
    },
    "logo": {
        "light": "./logo.png",
        "dark": "./logo.png"
    },
    "navbar": {
        "links": [],
        "primary": {
            "type": "button",
            "label": "Trade",
            "href": "https://t.me/tradewithgang_bot",
            "color": "#000000"
        }
    },
    "footer": {
        "socials": {
            "x": "https://x.com/tradewithgang",
            "telegram": "https://t.me/tradewithgang_bot",
            "discord": "https://discord.gg/tradewithgang"
        }
    }
}